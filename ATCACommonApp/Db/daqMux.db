record(bo, "$(DEVICE):DAQMUX$(CNT)_TRGDAQ")
{
    field(DESC, "DaqMux Trigger DAQ ($(CNT))")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) triggerDaq_$(CNT)")
    field(OMSL, "supervisory")
    field(SCAN, "Passive")
    field(ZNAM, "Normal")
    field(ONAM, "Trigger")
}

