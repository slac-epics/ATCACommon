            { "$(PORT)", "$(DEV)", "Int32",  1,  "ULONG", "asynInt32ArrayOut",   "ULONG",  "$(NELM\=2048)", "$(TSE)"    }
            { "$(PORT)", "$(DEV)", "Int32",  2,  "ULONG", "asynInt32ArrayOut",   "ULONG",  "$(NELM\=2048)", "$(TSE)"    }
            { "$(PORT)", "$(DEV)", "Int32",  3,  "ULONG", "asynInt32ArrayOut",   "ULONG",  "$(NELM\=2048)", "$(TSE)"    }

}

