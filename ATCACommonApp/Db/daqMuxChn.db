record(longout, "$(DEVICE):DAQMUX$(CNT)_INPMUXSEL$(CHN)")
{
    field(DESC, "DaqMux $(CNT) Input Mux Selector for CH$(CHN)")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) inputMuxSelect_$(CNT)_$(CHN)")
    field(SCAN, "Passive")
    field(PINI, "YES")
    info(autosaveFields, "VAL")
}

record(waveform, "$(DEVICE):DAQMUX$(CNT)_INPMUXSELSTR$(CHN)")
{
    field(DESC, "DaqMux $(CNT) Input Mux Selector for CH$(CHN)")
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn($(PORT),0) readInpMuxSel_$(CNT)_$(CHN)")
    field(SCAN, "I/O Intr")
    field(FTVL, "CHAR")
    field(NELM, "16")
}

record(bo, "$(DEVICE):DAQMUX$(CNT)_DECIMATION$(CHN)")
{
    field(DESC, "DaqMux $(CNT) Enable Decimation for CH$(CHN)")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),0) enableDecimation_$(CNT)_$(CHN)")
    field(OMSL, "supervisory")
    field(SCAN, "Passive")
    field(ZNAM, "Disable")
    field(ONAM, "Enable")
    field(PINI, "YES")
    field(VAL,  "0")
    info(autosaveFields, "VAL")
}
